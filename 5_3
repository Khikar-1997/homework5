package com.company;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int number1 = scanner.nextInt();
        System.out.println("number1 = " + number1);
        int number2 = scanner.nextInt();
        System.out.println("number2 = " + number2);
        int result = calculateProduct(number1, number2);
        double resultCube = calculateCube(result);
        System.out.println(resultCube);
    }

    public static int calculateProduct(int number1, int number2) {
        int product = number1 * number2;
        return product;
    }

    public static double calculateCube(double number) {
        double cube = Math.pow(number, 3);
        return cube;
    }
}